<?xml version="1.0"?>
<launch>
    <node pkg="usb_cam" exec="usb_cam_node_exe" output="screen">
        <param name="video_device" value="/dev/video0"/>
        <param name="image_width" value="320"/>
        <param name="image_height" value="240"/>
        <param name="framerate" value="10.0"/>
    </node>
</launch>
